@inherits Fluxor.Blazor.Web.Components.FluxorComponent

<div>
  @foreach (var rank in this.Board.Cells())
  {
    <div>
      @foreach (var cell in rank)
      {
        <CellComponent
          Cell="@cell"
          ShouldHighlight="@this.HighlightedCells.Contains(cell)" />
      }
    </div>
  }
</div>