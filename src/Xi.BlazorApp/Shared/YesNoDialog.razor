<MudDialog>
  <TitleContent>
    <MudText Typo="Typo.h6">
      <MudIcon Icon="far fa-question-circle" Class="mr-3 mb-n1"/>
      @this.Title
    </MudText>
  </TitleContent>
  <DialogContent>
    <MudText>@this.Content</MudText>
  </DialogContent>
  <DialogActions>
    <MudButton Color="@this.NoColor" OnClick="@this.No">@this.NoText</MudButton>
    <MudButton Color="@this.YesColor" OnClick="@this.Yes">@this.YesText</MudButton>
  </DialogActions>
</MudDialog>
